@model scopewebsite.Models.Class1;
<form asp-controller="Contact" asp-action="page5" style="margin-top:100px;">
    <label asp-for="Name">Name : </label>
    <input type="text" asp-for="Name" placeholder="Enter your name" />
    <span class="text-danger" asp-validation-for="Name"></span><br><br>

    <label asp-for="Email">Email : </label>
    <input type="email" asp-for="Email" placeholder="Enter your email" />
    <span class="text-danger" asp-validation-for="Email"></span><br><br>

    <label asp-for="Subject">Subject : </label>
    <input type="text" asp-for="Subject" placeholder="Enter your subject" />
    

    <label asp-for="Message">Message : </label>
    <input type="text" asp-for="Message" placeholder="Enter a message" />
    

    <input  type="submit" value="srnd email button"/>
</form>